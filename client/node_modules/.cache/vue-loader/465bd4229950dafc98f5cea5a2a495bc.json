ata: function() {\n    return {\n      name: null,\n      profession: null\n    };\n  },\n  methods: {\n    postCharacter: function() {\n      axios.post(\"https://gwz-easy.herokuapp.com/characters\", {\n        name: this.name,\n        profession: this.profession\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n</style>\n"]}]}