\n        name: 'CharacterCreator',\n        data: function () {\n            return {\n                name: null,\n                profession: null\n            }\n        },\n        methods: {\n            postCharacter: function () {\n                axios\n                    .post('http://localhost:3000/characters', {\n                        name: this.name,\n                        profession: this.profession\n                    });\n            }\n        }\n    }\n</script>\n\n<style scoped>\n</style>\n"]}]}